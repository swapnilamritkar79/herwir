<div class="row iomadlogonone"  >
    <div class="col-2">
        <img class="img-responsive" src="{{{ logourl }}}" alt="Iomad logo">
    </div>
    <div class="col-10 p-t-2">
        <h1 id="dashboardheader">{{# str }}iomaddashboard, block_iomad_company_admin{{/ str }}</h1>
    </div>
</div>

<div class="company-selection-hold">
   {{# companyselect.onecompany }}
       <div class="alert alert-info">
           {{#str }}currentcompany, block_iomad_company_admin,{{ companyselect.companyname }}{{/str }}
       </div>
   {{/ companyselect.onecompany }}
   {{^ companyselect.onecompany }}
       <div class="col-lg-12 col-md-12">
           {{{ companyselect.selectform }}}
		   {{{ companyselect.suspended }}}
       </div>
       
           
       
   {{/ companyselect.onecompany }}
</div>



<div class="m-t-2">
    {{> block_iomad_company_admin/tabs }}
    <div class="tab-content" id="iomadDashboardTabContent">
        {{# panes }}
            <div class="tab-pane fade1 {{# selected }}show1 active{{/ selected }} pt-3 iomadlink_container" id="{{ category }}" role="tabpanel" aria-labelledby="{{ category }}-tab">
                {{> block_iomad_company_admin/menuitem }}
                <div class="clearfix"></div>
            </div>
        {{/ panes }}
    </div>
</div>


